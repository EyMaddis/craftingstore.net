@font-face {
	font-family: 'VT323';
	font-style: normal;
	font-weight: normal;
	src: local('VT323'), url(./fonts/VT323.woff) format('woff');
}
* {
	padding:0;
	margin:0; 
}

html, body {
	width: 100%;
	height: 100%;
}
body {
	background: #FFFFFF url(./images/background-x.jpg) repeat-x top left;
	font-family: arial,helvetica,sans-serif;
	/*text-shadow:0 1px 2px #CCD4DD;*/
}
table {
	border-spacing: 0;
}
a:active, a:visited, a {
	color: #567;
	text-decoration: none; 
}
.wrapper {
	background: transparent url(./images/background-y.jpg) repeat-y top center;
}
.background {
	background: transparent url(./images/background.jpg) no-repeat top center;
}
.page {
	background: #FFF;
	/*border: solid 1px #AA0000;*/
	width: 1024px;
	margin: auto;
}
.header {
	/*border: solid 1px #AAAA00;
	height: 130px;*/
	border-bottom: solid 1px #CCCCCC;
	width: 100%;
}
.lmenu {
	/*border: solid 1px #AAAAAA;*/
	padding: 0 10px;
	float: left;
	width: 200px;
	height: 100%;
	margin-top: 15px;
}
.main {
	padding: 0 10px;
	margin-top: 16px;
	float: left;
	width: 560px; /* Without the border: 560px!*/
	/*border: solid 1px #EEE;*/
}

.Right {
	padding: 0 10px;
	float: right;
	width: 200px;
	height: 100%;
	/*border: solid 1px #AAAAAA;*/
	margin-top: 15px;
}
.right > div{
	margin-bottom: 4px;
}


.overallMessage {
	width: 500px;
	height: 100px;
	padding: 0.4em;
	position: relative;
	background: url(./images/icons/exclamation.png) no-repeat 40px 20px;
	display: none;
	
}
.overallMessage h3 { 
	margin: 0; 
	padding: 0.4em; 
	text-align: center; 
}
.overallMessage p { 
	padding: 1em 0.4em 0.4em 100px;
	font-weight: bold;
}
/* HEADER */
.header {
	padding-top: 25px;
}
.header .shopinfo {
	/*border: solid 1px red;
	width: 250px;
	height: 80px;
	float: left;*/
	margin: 25px auto;
}
.header .shopinfo .logo {
	color: red;
	font-size: 18px;
	text-align: center;
}
.shopinfo .logo img{
	border: 0;
	max-width: 1000px;
	max-height: 125px;
}
.header .shopinfo .shopdesc {
	position: relative;
	top: -10px;
	margin-left: 20px; 
}
.header .topmenu {
	clear: right;
	float:right;
	top: -28px;
	margin: 0 8px -28px;
	position: relative;
}
.header .topmenu ul li{
	list-style: none;
	display:inline;
}

.header .topmenu  ul li a{
	/*display: inline;
	border: solid 1px grey;
	padding: 5px 10px;*/
	border-radius: 6px 6px 0 0;
	-moz-border-radius: 6px 6px 0 0; 
	-webkit-border-radius: 6px 6px 0 0; 
	-o-border-radius: 6px 6px 0 0; 
	-khtml-border-radius: 6px 6px 0 0; 
	border-bottom: none;
}

.footer {
	overflow:auto;
	padding:25px;
	width: 975px;
	background: #4A4A4A url(./images/footer.jpg) repeat-x;
	color: #FFF;
	margin-top: 10px;
}
.footer a{
	color: #fff;
	font-style:italic;
}
.footer > table{
	width:100%;
	margin-top:5px;
}
.footer .langchanger {
    float:right;
	 overflow:auto;
}


/* MAIN */
.clear {
	clear:both;
}

.AjaxLoaderImg {
	width: 220px;
	height: 19px;
	margin: 30px auto;
	background: transparent url(./images/loading_bar.gif) no-repeat top left;
}
.AjaxLoaderImg span {
	display:none;
}

.bonuspoints {
	color: green;
	cursor: help;
}

.blueButton{
	background: #1b64dc url(./images/blue_gradient.png) repeat-x center center;
	font-weight: bold;
	font-size: 13px;
	padding: 2px 10px;
}
.greyButton{
	background: #aaa url(./images/grey_gradient.png) repeat-x center center;
	font-size: 13px;
}
.blueButton,
.greyButton{
	padding:3px 10px;
	color:#FFF;
	text-decoration: none;
	height: 26px;
	border:solid 1px #ccc;
	border-radius:4px;
	white-space:pre-line;
	text-align:center;
}
input.blueButton,
input.greyButton{
	cursor:pointer;
}
div>a.blueButton,
div>a.greyButton{
